{
  "name": "evidently-ui-lib",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "postinstall": "pnpm build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "exports": {
    "./routes-components/*/data": {
      "default": "./src/routes-components/*/data.tsx"
    },
    "./routes-components/*": {
      "default": "./src/routes-components/*/index.tsx"
    },
    "./*": {
      "default": "./src/*",
      "types": [
        "./.tsc-dts/src/*/index.d.ts",
        "./.tsc-dts/src/*.d.ts"
      ]
    }
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-table/core": "^6.2.11",
    "@mui/icons-material": "^5.14.12",
    "@mui/material": "^5.14.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "tiny-invariant": "^1.3.1",
    "react-hook-form": "^7.47.0",
    "@hookform/resolvers": "^3.3.1",
    "zod": "^3.22.4",
    "dayjs": "^1.11.10",
    "react-router-dom": "^6.16.0",
    "@nivo/bar": "^0.83.0",
    "@nivo/line": "^0.83.0",
    "@nivo/scatterplot": "^0.83.0",
    "@nivo/core": "^0.83.0",
    "@material-table/core": "^6.2.11",
    "plotly.js-cartesian-dist-min": "^2.27.1",
    "plotly.js": "^2.27.1",
    "react-plotly.js": "^2.6.0",
    "react-markdown": "^9.0.0",
    "@mui/x-date-pickers": "^6.16.2"
  },
  "devDependencies": {
    "@types/plotly.js-cartesian-dist-min": "npm:@types/plotly.js-dist-min@^2.3.4",
    "@types/plotly.js": "^2.12.30",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-plotly.js": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2"
  }
}
